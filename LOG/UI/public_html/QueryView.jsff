        <af:commandButton actionListener="#{bindings.ExecuteWithParams.execute}" text="Search"
                          disabled="#{!bindings.ExecuteWithParams.enabled}" id="cb2" action="#{querybean.getStats}"/>
        <af:spacer width="10" height="10" id="s1"/>
        <af:outputText id="ot31" inlineStyle="font-size:medium;" binding="#{querybean.statsmsg}"/>
        <af:outputText id="ot32" inlineStyle="font-size:medium;" binding="#{querybean.statsmsg1}"/>
        <af:panelFormLayout id="pfl1" rendered="false">
          <af:panelLabelAndMessage label="#{bindings.Active.hints.label}" id="plam1">
            <af:outputText value="#{bindings.Active.inputValue}" id="ot11"/>
          </af:panelLabelAndMessage>
          <af:panelLabelAndMessage label="#{bindings.DistributionRole.hints.label}" id="plam2">
            <af:outputText value="#{bindings.DistributionRole.inputValue}" id="ot12">
              <af:convertNumber groupingUsed="false" pattern="#{bindings.DistributionRole.format}"/>
            </af:outputText>
          </af:panelLabelAndMessage>
          <af:panelLabelAndMessage label="#{bindings.FacAddress1.hints.label}" id="plam3">
            <af:outputText value="#{bindings.FacAddress1.inputValue}" id="ot13"/>
