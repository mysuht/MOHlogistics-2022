<?xml version='1.0' encoding='windows-1256'?>
<ui:composition xmlns:ui="http://java.sun.com/jsf/facelets" xmlns:af="http://xmlns.oracle.com/adf/faces/rich"
                xmlns:f="http://java.sun.com/jsf/core">
  <af:panelSplitter id="ps1" orientation="vertical" splitterPosition="141">
    <f:facet name="first">
      <af:panelGroupLayout layout="scroll" id="pgl1">
        <af:selectOneChoice value="#{bindings.p_year.inputValue}" label="#{bindings.p_year.label}"
                            required="#{bindings.p_year.hints.mandatory}" shortDesc="#{bindings.p_year.hints.tooltip}"
                            id="soc1" binding="#{querybean.year}">
          <f:selectItems value="#{bindings.p_year.items}" id="si1"/>
        </af:selectOneChoice>
        <af:selectOneChoice value="#{bindings.p_month.inputValue}" label="#{bindings.p_month.label}"
                            required="#{bindings.p_month.hints.mandatory}" shortDesc="#{bindings.p_month.hints.tooltip}"
