                   sortable="true" headerText="#{bindings.CentersLOVDirectoratesLOV.hints.FacPhone.label}" id="c8"
                   width="105">
          <af:outputText value="#{row.FacPhone}" id="ot8"/>
        </af:column>
        <af:column sortProperty="#{bindings.CentersLOVDirectoratesLOV.hints.FacRescode.name}" filterable="true"
                   sortable="true" headerText="#{bindings.CentersLOVDirectoratesLOV.hints.FacRescode.label}" id="c9"
                   width="105">
          <af:outputText value="#{row.FacRescode}" id="ot9"/>
        </af:column>
      </af:table>
    </f:facet>
  </af:panelSplitter>
</ui:composition>
