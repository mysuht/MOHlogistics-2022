            </af:outputText>
          </af:panelLabelAndMessage>
          <af:panelLabelAndMessage label="#{bindings.FacilityId.hints.label}" id="plam14">
            <af:outputText value="#{bindings.FacilityId.inputValue}" id="ot24" binding="#{querybean.facid}">
