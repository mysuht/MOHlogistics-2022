                filterModel="#{bindings.CentersLOVDirectoratesLOVQuery.queryDescriptor}"
                queryListener="#{bindings.CentersLOVDirectoratesLOVQuery.processQuery}" filterVisible="true"
                varStatus="vs" selectedRowKeys="#{bindings.CentersLOVDirectoratesLOV.collectionModel.selectedRow}"
                selectionListener="#{bindings.CentersLOVDirectoratesLOV.collectionModel.makeCurrent}"
                rowSelection="single" id="t1" partialTriggers="::cb2">
        <af:column sortProperty="#{bindings.CentersLOVDirectoratesLOV.hints.Enterd.name}" filterable="true"
                   sortable="true" headerText="#{bindings.CentersLOVDirectoratesLOV.hints.Enterd.label}" id="c1"
