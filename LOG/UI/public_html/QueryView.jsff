          <f:selectItems value="#{bindings.p_month.items}" id="si2"/>
        </af:selectOneChoice>
        <af:selectOneChoice value="#{bindings.DirectoratesLOV.inputValue}" label="#{bindings.DirectoratesLOV.label}"
