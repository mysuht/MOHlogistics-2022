<?xml version="1.0" encoding="windows-1256" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="AdjTypeTF">
    <default-activity id="__1">AdjTypeBrowse</default-activity>
    <view id="AdjTypeBrowse">
      <page>/AdjTypeBrowse.jsff</page>
    </view>
    <view id="AdjTypeOperation">
      <page>/AdjTypeOperation.jsff</page>
    </view>
    <method-call id="CreateInsert">
      <method>#{bindings.CreateInsert.execute}</method>
      <outcome id="__2">
        <fixed-outcome>CreateInsert</fixed-outcome>
      </outcome>
    </method-call>
    <control-flow-rule id="__3">
      <from-activity-id id="__4">AdjTypeBrowse</from-activity-id>
      <control-flow-case id="__5">
        <from-outcome id="__7">insert</from-outcome>
        <to-activity-id id="__6">CreateInsert</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__9">
        <from-outcome id="__10">update</from-outcome>
        <to-activity-id id="__8">AdjTypeOperation</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__11">
      <from-activity-id id="__12">AdjTypeOperation</from-activity-id>
      <control-flow-case id="__13">
        <from-outcome id="__15">back</from-outcome>
        <to-activity-id id="__14">AdjTypeBrowse</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__16">
      <from-activity-id id="__17">CreateInsert</from-activity-id>
      <control-flow-case id="__18">
        <from-outcome id="__19">CreateInsert</from-outcome>
        <to-activity-id id="__20">AdjTypeOperation</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
