<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="GofTF">
    <default-activity>GofBrowse</default-activity>
    <view id="GofBrowse">
      <page>/GofBrowse.jsff</page>
    </view>
    <view id="GofOperations">
      <page>/GofOperations.jsff</page>
    </view>
    <method-call id="CreateInsert">
      <method>#{bindings.CreateInsert.execute}</method>
      <outcome>
        <fixed-outcome>CreateInsert</fixed-outcome>
      </outcome>
    </method-call>
    <control-flow-rule id="__1">
      <from-activity-id>GofBrowse</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>insert</from-outcome>
        <to-activity-id>CreateInsert</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__3">
        <from-outcome>update</from-outcome>
        <to-activity-id>GofOperations</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__4">
      <from-activity-id>CreateInsert</from-activity-id>
      <control-flow-case id="__5">
        <from-outcome>CreateInsert</from-outcome>
        <to-activity-id>GofOperations</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__6">
      <from-activity-id>GofOperations</from-activity-id>
      <control-flow-case id="__7">
        <from-outcome>back</from-outcome>
        <to-activity-id>GofBrowse</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
