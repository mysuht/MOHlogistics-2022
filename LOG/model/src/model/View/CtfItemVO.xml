        <Item Value="model.Entity.CtfItemEO.ProdId"/>
      </AttrArray>
      <AttrArray Name="_dstAttributes">
        <Item Value="model.Entity.ProductEO.ProdId"/>
      </AttrArray>
    </DesignTime>
  </EntityUsage>
  <ViewAttribute
