  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  FetchMode="FETCH_AS_NEEDED"
  OrderBy="PROD_ID"
  InheritPersonalization="merge">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <ViewAccessor
    Name="AdjTypeLOV"
    ViewObjectName="model.LOV.AdjTypeLOV"
    RowLevelBinds="true"/>
  <ViewAccessor
