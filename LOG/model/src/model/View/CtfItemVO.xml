    </AttrArray>
    <AttrArray Name="ListDisplayAttrNames">
      <Item Value="TypeName"/>
    </AttrArray>
    <DisplayCriteria/>
  </ListBinding>
  <SQLQuery><![CDATA[SELECT CtfItemEO.CTF_ITEM_ID,    
       CtfItemEO.CTF_MAIN_ID,    
       CtfItemEO.P_DATE,    
       CtfItemEO.PROD_ID,    
      nvl( CtfItemEO.OPEN_BAL,0),    
      nvl( CtfItemEO.RECEIPTS, 0),   
      nvl( CtfItemEO.ISSUES, 0),   
     nvl(  ABS(CtfItemEO.ADJUSTMENTS),  0),  
       CtfItemEO.ADJ_TYPE_ID,    
     nvl(  CtfItemEO.CLOSING_BAL,   0), 
       CtfItemEO.ERR_OB,    
       CtfItemEO.ERR_CB,    
       CtfItemEO.DE_STAT,    
       CtfItemEO.NEW_VISITS,    
       CtfItemEO.CONT_VISITS,    
       CtfItemEO.ERR_QTY_RQRD,    
       CtfItemEO.ERR_QTY_RCVD,    
       CtfItemEO.ERR_AMC,    
       CtfItemEO.AVG_MNTHLY_CONS,    
       CtfItemEO.QTY_REQUIRED,    
       CtfItemEO.QTY_RECEIVED,    
       AdjTypeEO.TYPE_NAME,    
       AdjTypeEO.ADJ_TYPE_ID AS ADJ_TYPE_ID1,    
       ProductEO.PRO_NAME,    
       ProductEO.PROD_ID AS PROD_ID1,    
       ProductEO.PRO_DOSE   
FROM  CTF_ITEM CtfItemEO, ADJ_TYPE AdjTypeEO, PRODUCT ProductEO   
WHERE (CtfItemEO.ADJ_TYPE_ID = AdjTypeEO.ADJ_TYPE_ID(+)) AND (CtfItemEO.PROD_ID = ProductEO.PROD_ID(+))]]></SQLQuery>
  <EntityUsage
    Name="CtfItemEO"
    Entity="model.Entity.CtfItemEO"/>
  <EntityUsage
