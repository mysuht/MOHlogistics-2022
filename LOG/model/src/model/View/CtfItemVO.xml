    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="OpenBal"
    PrecisionRule="true"
    EntityAttrName="OpenBal"
    EntityUsage="CtfItemEO"
    AliasName="OPEN_BAL"
    DefaultValue="0"/>
  <ViewAttribute
    Name="Receipts"
    PrecisionRule="true"
    EntityAttrName="Receipts"
    EntityUsage="CtfItemEO"
    AliasName="RECEIPTS"
    DefaultValue="0"/>
  <ViewAttribute
