    </DesignTime>
  </EntityUsage>
  <EntityUsage
    Name="ProductEO"
    Entity="model.Entity.ProductEO"
    Association="model.Asso.FkCtfItemProductASSO"
    AssociationEnd="model.Asso.FkCtfItemProductASSO.Product"
    SourceUsage="model.View.CtfItemVO.CtfItemEO"
    ReadOnly="true"
    Reference="true"
    DeleteParticipant="false"
    JoinType="LEFT OUTER JOIN">
    <DesignTime>
