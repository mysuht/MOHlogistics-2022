    Entity="model.Entity.AdjTypeEO"
    Association="model.Asso.FkCtfItemAdjASSO"
    AssociationEnd="model.Asso.FkCtfItemAdjASSO.AdjType"
    SourceUsage="model.View.CtfItemVO.CtfItemEO"
    ReadOnly="true"
    Reference="true"
    DeleteParticipant="false"
    JoinType="LEFT OUTER JOIN">
    <DesignTime>
      <AttrArray Name="_srcAttributes">
        <Item Value="model.Entity.CtfItemEO.AdjTypeId"/>
      </AttrArray>
      <AttrArray Name="_dstAttributes">
        <Item Value="model.Entity.AdjTypeEO.AdjTypeId"/>
