      <Item Value="ProdId"/>
    </AttrArray>
    <AttrArray Name="ListDisplayAttrNames">
      <Item Value="ProdId"/>
      <Item Value="ProName"/>
    </AttrArray>
    <DisplayCriteria
      Hint="showAndExecute"/>
  </ListBinding>
  <ListBinding
    Name="LOV_AdjTypeId"
    ListVOName="AdjTypeLOV"
    ListRangeSize="-1"
