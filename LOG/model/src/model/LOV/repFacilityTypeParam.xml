<?xml version="1.0" encoding="windows-1256" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="repFacilityTypeParam"
  Version="12.1.2.66.68"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  OrderBy="2"
  FetchMode="FETCH_ALL">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.LOV.repFacilityTypeParam_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="sup"
    Kind="where"
    Type="java.lang.Integer"
    DefaultValue="6"/>
  <SQLQuery><![CDATA[select facility_id , nvl(fac_name,' ') name, fac_code from   
facility where fac_type_id in (:sup)  and   
(   
select count(*) from facility where fac_type_id in (:sup)   
) > 0   
   
union   
   
select facility_id , nvl(fac_name,' '), fac_code from   
facility where fac_type_id in   
(select gof_facility_id from group_of_facilities where   
gof_grp_id = ( :sup - 100 ) )   
and   
(   
select count(*) from facility where fac_type_id in (:sup)   
) <= 0]]></SQLQuery>
  <ViewAttribute
    Name="FacilityId"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="7"
    Scale="0"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="FACILITY_ID"
    Expression="FACILITY_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="Name"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="NAME"
    Expression="NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="FacCode"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="20"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="FAC_CODE"
    Expression="FAC_CODE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="FacilityId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.modelBundle"/>
  </ResourceBundle>
</ViewObject>
