  <SQLQuery><![CDATA[SELECT   
decode((select count(*) from ctf_main where facilityeo.facility_id = ctf_main.facility_id and to_char(p_date,'yyyy') = :p_year and to_char(p_date,'mm')=:p_month ),0,'Not Initialized',1,'Initialized','Initialized') enterd,  
(select decode(ctf_stage,'O','Open','A','Approved')  from ctf_main where facilityeo.facility_id = ctf_main.facility_id and to_char(p_date,'yyyy') = :p_year and to_char(p_date,'mm')=:p_month) approved,  
FacilityEO.ACTIVE,   
       FacilityEO.DISTRIBUTION_ROLE,   
       FacilityEO.FAC_ADDRESS1,   
       FacilityEO.FAC_ADDRESS2,   
       FacilityEO.FAC_CITY,   
       FacilityEO.FAC_CODE,   
       FacilityEO.FAC_CONTACT,   
       FacilityEO.FAC_FAX,   
       FacilityEO.FAC_NAME,   
       FacilityEO.FAC_PHONE,   
       FacilityEO.FAC_RESCODE,   
       FacilityEO.FAC_STATE,   
       FacilityEO.FAC_TYPE_ID,   
       FacilityEO.FACILITY_ID,   
       FacilityEO.FIELD_DISPENSE,   
       FacilityEO.HIGHEST_LV,   
       FacilityEO.MAX_MOS,   
       FacilityEO.MIN_MOS,   
       FacilityEO.SUP_CODE,   
       FacilityEO.WAREHOUSE  
FROM FACILITY FacilityEO  
 GROUP BY FacilityEO.ACTIVE, FacilityEO.DISTRIBUTION_ROLE, FacilityEO.FAC_ADDRESS1, FacilityEO.FAC_ADDRESS2, FacilityEO.FAC_CITY, FacilityEO.FAC_CODE, FacilityEO.FAC_CONTACT, FacilityEO.FAC_FAX, FacilityEO.FAC_NAME, FacilityEO.FAC_PHONE, FacilityEO.FAC_RESCODE, FacilityEO.FAC_STATE, FacilityEO.FAC_TYPE_ID, FacilityEO.FACILITY_ID, FacilityEO.FIELD_DISPENSE, FacilityEO.HIGHEST_LV, FacilityEO.MAX_MOS, FacilityEO.MIN_MOS, FacilityEO.SUP_CODE, FacilityEO.WAREHOUSE]]></SQLQuery>
